pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
local hopper = {}
local rocket = {}
local mrbombs = {}
local settings = {}

local hitbox = {}

local flag1 = {}
local flag2 = {}
local target_flag

local score

function _init()
 cls()
	palt(0,false) --disable black transarency
	mapdraw(0,0,0,12,16,10)
	
	settings.difficulty = 2
	
	--setup our hopper
	hopper.x = 60
	hopper.y = 67
	hopper.speed = 3
	hopper.sprite = 1
	
	--setup our rocket
	rocket.x = 10
	rocket.y = 14
	rocket.direction = 1
	rocket.speed = 2
	rocket.sprite = 14
	
	--set up the flags
	flag1.x = 13
	flag1.y = 67
	flag1.sprite = 12
	
	flag2.x = 110
	flag2.y = 67
	flag2.sprite = 13

	target_flag = flag1
	
	-- init score to 0
	score = 0

	spr(1, hopper.x, hopper.y)
end

function _draw()
 cls()
 
 map(0,0,0,12,16,10)
	
	spr(flag1.sprite, flag1.x, flag1.y)
	spr(flag2.sprite, flag2.x, flag2.y)

	
	if(btnp(1)) then
		hopper.x += hopper.speed
	end
	
	if(btnp(0)) then
		hopper.x -= hopper.speed
	end
	
	spr(hopper.sprite, hopper.x, hopper.y)
	spr(rocket.sprite, rocket.x, rocket.y)

 
 --move rocket back and forth.
 if(rocket.x > 105 or rocket.x < 2) then 
  rocket.direction *= -1
 end 
 rocket.x += (rocket.direction*rocket.speed)

 --drop a bomb, maybe.
 drop_bomb = rnd(100)
	if(drop_bomb < 10) then
	 local b = {}
	 b.sprite = 11
	 b.x = rocket.x
	 b.y = rocket.y
	 b.speed = settings.difficulty
		add(mrbombs, b)
	end

	for bomb in all(mrbombs) do
		bomb.y += bomb.speed
		
		if(bomb.y > 67) then
			-- explode bomb?
			sfx(0)
			del(mrbombs, bomb)
		end
		
		-- draw bomb
		spr(bomb.sprite,bomb.x, bomb.y) 
		
		if(bomb.y >= hopper.y and bomb.y <= hopper.y+8 
				and bomb.x >= hopper.x and bomb.x <= hopper.x+8) then
				-- we hit the hopper!
					sfx(8)
 	end
	end
	
	--detect if hopper hits the target flag
	if(abs(hopper.x - target_flag.x) < 5) then
		sfx(3)
		score += 1
		if(target_flag == flag1) then
			target_flag = flag2
		else 
			target_flag = flag1 
		end
	end		
	

	print(score)
end

