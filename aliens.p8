pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
local state = "" -- our games state
local hopper = {}
local rocket = {}
local mrbombs = {}
local settings = {}
local lasers = {}
local missles = {}

local hitbox = {}

local flag1 = {}
local flag2 = {}
local target_flag

local score
local life

function _init()
    cls()
	palt(0,false) --disable black transarency

	state = "title"
end

function _update()
 if(state=="title") then
  do_update_title()
 elseif(state=="play") then
  --do_update_play()
 end
end

function do_update_title()
 if(btnp(4)) then
  game_init()
  state = "play"
 end
end


function draw_title()
 cls()
 print("aliens",55,50,7)
 print("capture the highlighted",20,70,9)
 print("flag for points!", 35, 78, 9)
 print("z to start",46,89,11)
end

function game_init()
	mapdraw(0,0,0,12,16,10)
	
	settings.difficulty = 2
	
	--setup our hopper
	hopper.x = 60
	hopper.y = 67
	hopper.speed = 5
	hopper.sprite = 1
	hopper.alive = 1
	hopper.facing = 1 -- facing right
	
	--setup our rocket
	rocket.x = 10
	rocket.y = 14
	rocket.direction = 1
	rocket.speed = 2
	rocket.sprite = 14
	
	--set up the flags
	flag1.x = 13
	flag1.y = 67
	flag1.sprite = 12
	
	flag2.x = 110
	flag2.y = 67
	flag2.sprite = 13

	target_flag = flag1
	
	-- init score to 0 and our life to 100
	score = 0
	life = 3

	spr(1, hopper.x, hopper.y)

end

function _draw()
 cls()

 if(state == "title") then
	 draw_title()
	 return
 end
 
 map(0,0,0,12,16,10)
	
	spr(flag1.sprite, flag1.x, flag1.y)
	spr(flag2.sprite, flag2.x, flag2.y)

	if(btnp(1) and hopper.alive == 1) then
		hopper.x += hopper.speed
		hopper.facing = 1
	end
	
	if(btnp(0) and hopper.alive == 1) then
		hopper.x -= hopper.speed
	 hopper.facing = 0
	end
	
	if(btnp(4) and hopper.alive == 1) then
		m = {}
		m.x = hopper.x
		m.y = hopper.y-10
		m.sprite = 40
		
		sfx(10)
		add(missles, m)
	end
	
	if(btnp(5) and hopper.alive == 1) then
		l= {}
		l.x = hopper.x
		l.y = hopper.y
		l.sprite = 27
		l.dx = 2 

		if(hopper.facing == 0) then
			l.dx = -2
		 l.x = l.x - 4
		else
		 l.x = l.x + 4
		end
		
		sfx(7)
		add(lasers, l)
	end
	
	--draw our character
	if(life > 0) then
		hopper.sprite = 1
	else
	 hopper.sprite = 34 -- our dead hopper
	end
	
	spr(hopper.sprite, hopper.x, hopper.y)
	spr(rocket.sprite, rocket.x, rocket.y)

 
 --move rocket back and forth.
 if(rocket.x > 105 or rocket.x < 2) then 
  rocket.direction *= -1
 end 
 rocket.x += (rocket.direction*rocket.speed)

 --drop a bomb, maybe.
 drop_bomb = rnd(100)
	if(drop_bomb < 10) then
	 local b = {}
	 b.sprite = 11
	 b.x = rocket.x
	 b.y = rocket.y
	 b.speed = settings.difficulty
		add(mrbombs, b)
	end

	for l in all(lasers) do
		spr(l.sprite,l.x, l.y)
		l.x += l.dx
	end
	
	for m in all(missles) do 
		spr(m.sprite, m.x, m.y)
		m.y -= 3
	end

	for bomb in all(mrbombs) do
		bomb.y += bomb.speed
		
		if(bomb.y > 67) then
			-- explode bomb?
			sfx(0)
			del(mrbombs, bomb)
		end
		
		-- draw bomb
		spr(bomb.sprite,bomb.x, bomb.y) 
		
		if(bomb.y >= hopper.y and bomb.y <= hopper.y+8 
				and bomb.x >= hopper.x and bomb.x <= hopper.x+8) then
				 -- we hit the hopper!
					sfx(8)

					-- decrease life by 1
					life -= 1
 	end
	end
	
	--detect if hopper hits the target flag
	if(abs(hopper.x - target_flag.x) < 5) then
		sfx(3)
		score += 1
		if(target_flag == flag1) then
			target_flag = flag2
		else 
			target_flag = flag1 
		end
	end		

	if(life <= 0) then
		print("game over", 45, 45)
		-- sfx(21)
		life = 0 -- so don't go negative
	 hopper.alive = 0
	end
	
	print("score: ", 0, 0)
	print(score, 28, 0)
	
	print("life: ", 100, 0)
	print(life, 120, 0)
end

